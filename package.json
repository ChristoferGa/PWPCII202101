{
  "name": "projnotes",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "SET NODE_ENV = production & node ./dist-server/bin/www",
    "watch:dev": "SET NODE_ENV = production & babel-watch ./server/bin/www",
    "webpack:ser": "SET NODE_ENV= development & webpack serve --config ./webpack.dev.config.js --mode development",
    "cleans": "rimraf dist-server",
    "cleanc": "rimraf public/js",
    "build:ser": "npm run cleans && babel ./server --out-dir dist-server --copy-files",
    "build:cli": "npm run cleanc && webpack --config ./webpack.config.js --mode production",
    "build": "npm run build:cli && npm run build:ser"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "hbs": "^4.1.2",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/node": "^7.14.2",
    "@babel/preset-env": "^7.14.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-preset-minify": "^0.5.1",
    "babel-watch": "^7.4.1",
    "corejs": "^1.0.0",
    "css-loader": "^5.2.6",
    "eslint": "^7.28.0",
    "eslint-webpack-plugin": "^2.5.4",
    "mini-css-extract-plugin": "^1.6.0",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-middleware": "^5.0.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-hot-middleware": "^2.25.0"
  }
}
