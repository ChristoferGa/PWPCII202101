{
  "name": "projnotes",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "SET NODE_ENV = production & node ./dist-server/bin/www",
    "watch:dev": "SET NODE_ENV = production & babel-watch ./server/bin/www",
    "clean": "rimraf dist-server",
    "build:ser": "npm run clean && babel ./server --out-dir dist-server --copy-files",
    "webpack:ser": "SET NODE_ENV= development & webpack serve --config ./webpack.config.js --mode development"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
